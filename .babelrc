{
  "presets": [["env", { "modules": false }]],
  "plugins": [
    "transform-es2017-object-entries",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "transform-es2017-object-entries",
        "transform-object-rest-spread",
        "external-helpers"
      ]
    }
  }
}
